<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Application" max_length="40" name="x_snc_appmgr_application" type="collection">
        <element default="true" label="Active" max_length="40" name="active" type="boolean"/>
        <element label="Description" max_length="1000" name="description" type="string"/>
        <element display="true" label="Name" max_length="40" name="name" type="string"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element default="100" label="Order" max_length="40" name="order" type="integer"/>
        <element label="Page suffix" max_length="40" name="page_suffix" type="string"/>
        <element label="Repository" max_length="1024" name="repository" type="url"/>
        <element label="Team" max_length="4000" name="team" reference="sys_user" type="glide_list"/>
    </element>
</database>
